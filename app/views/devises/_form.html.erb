<%= form_for(@devise) do |f| %>
    <% if @devise.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(@devise.errors.count, "error") %> prohibited this devise from being saved:</h2>

          <ul>
            <% @devise.errors.full_messages.each do |message| %>
                <li><%= message %></li>
            <% end %>
          </ul>
        </div>
    <% end %>

    <h2> Campos de Devise</h2>
    <div class="field">
      <%= f.label :password %><br>
      <%= f.text_field :password %>
    </div>
    <div class="field">
      <%= f.label :email %><br>
      <%= f.text_field :email %>
    </div>

    <h2>Campos de Put</h2>
    <%= f.fields_for :put do |p| %>
        <%= p.label :nombre %><br>
        <%= p.text_field :nombre %>
        <%= p.label :apellido %><br>
        <%= p.text_field :apellido %>

        <h2>Campos de U</h2>
        <%= p.fields_for :u do |u| %>
            <%= u.label :cargo %><br>
            <%= u.text_field :cargo %>
            <%= u.label :apodo %><br>
            <%= u.text_field :apodo %>

        <%end%>
    <%end %>



    <div class="actions">
      <%= f.submit %>
    </div>
<% end %>
